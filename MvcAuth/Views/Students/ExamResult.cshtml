@model IEnumerable<MvcAuth.Models.Exam_Result_Tbl>

@{ ViewBag.Title = "Exam Result";
                Layout = "~/Views/Shared/_LayoutStudent.cshtml"; }

<div class="container mt-5">
    <div class="row">
        <div class="col-md-12 ml-auto">

            <div class="panel text-center">
                    <h1 class="title" style="color:#ca3d26">Exam Results</h1>
                        <br />
                        <div class="table-responsive">
                            <table class="table table-striped ">
                                <tr>
                                    <td>
                                        Id
                                    </td>
                                    <td>
                                        Exam Name
                                    </td>
                                    <td>
                                        Student Name
                                    </td>
                                    <td>
                                        Degree
                                    </td>

                                </tr>
                                @if (Model.Count() == 0)
            {
        <tr>
            <td>No Data</td>
        </tr>
            }
            else
            {
                                                foreach (var item in Model)
                                {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Res_ID)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ExamTbl.Ex_Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.StudentTbl.St_Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Res_Degree)
                                    </td>

                                </tr>}

                                }
                            </table>
                        </div>
            </div>
        </div>
    </div>
</div>