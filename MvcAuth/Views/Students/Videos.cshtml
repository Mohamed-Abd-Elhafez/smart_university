
@model IEnumerable<MvcAuth.Models.Prof_Video_TBL>
@{ ViewBag.Title = "Videos";
    Layout = "~/Views/Shared/_LayoutStudent.cshtml"; }
@if (Model.Count() <= 0)
{
<div class="card-body mb-5 ">
    <div class="card-header ">
        No Videos
    </div>
</div> }
            else
            {
<div class="container mt-5">
    <div class="row">
        @foreach (var item in Model)
        {
<div class="col-md-4">
    <div class="card mb-5 mr-6" style="margin: auto; width: 18rem;">
        <div class="card-body">
            <h5 class="card-header">
                @Html.DisplayFor(modelItem => item.Prof_Video_Name)
            </h5>
            <div class="card-header">
                @Html.DisplayFor(modelItem => item.CourseTbl.Course_Name)
            </div>
        </div>
        <div class="card-footer text-center">
            <a href="@item.Prof_Video">Open</a>
        </div>
    </div>


</div>}
    </div>

</div>}