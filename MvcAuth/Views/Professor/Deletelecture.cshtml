@model MvcAuth.Models.Prof_Lecture_TBL

@{
    ViewBag.Title = "Deletelecture";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <h3>Are you sure you want to delete this ?</h3>
    <div>
       
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.HiddenFor(model => model.Prof_Lecture_ID)
            </dt>
          

            <dt>
                @Html.DisplayName("Lecture name ")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Prof_Lecture_Name)
            </dd>

          

            <dt>
                @Html.DisplayName("Course name")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CourseTbl.Course_Name)
            </dd>

        </dl>



        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "FileUpload")
        </div>
     
    </div>
}
